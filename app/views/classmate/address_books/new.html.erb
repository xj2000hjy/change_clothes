<% content_for :style_link do %>
<style type="text/css">
  #reg_address{
      width: 400px;
      margin: 0 auto;
      background-color: #eee;
      text-align: center;
      padding: 3px;
  }
  h1{
      height: 25px;
      line-height: 25px;
  }
</style>
<%end%>

<div id="reg_address">
  <h1>注册信息</h1>
  <%= form_for @address_book, url:{action: address_books_path}, html:{method: :post,multipart: true, authenticity_token: false} do |f|%>
  <table>
    <tr>
        <td align="right"><%= f.label '登录用户名:', :ursername %></td><td align="left"><%= f.text_field :username %></td>
    </tr>
  </table>
  <% end %>
</div>